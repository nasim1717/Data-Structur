/// Lab task 1 qus no 1


#include<bits/stdc++.h>
using namespace std;
int add(int x,int y){
    int sum;
    sum = x+y;
return sum;
}
int sub(int x, int y){
    int subtraction;
    subtraction = x-y;
return subtraction;
}
int max(int x, int y){
    if(x>y)
        return x;
    else
        return y;
}
int min(int x, int y){
    if(x<y)
        return x;
    else
        return y;
}
int main(){
    int a,b,sum;
    cin>>a>>b;
    cout<< "add = "<<add(a,b)<<endl;
    cout<<"Sub = "<<sub(a,b)<<endl;
    cout<<"max = "<<max(a,b)<<endl;
    cout<<"min = "<<min(a,b)<<endl;
return 0;
}
