#include<bits/stdc++.h>
using namespace std;
void print_arry(int a[], int len){
    a[2]= 5;
}
main(){
    int i,x,n,arr[200];


    cin>>n;
    for(i=0; i<n; i++){
       cin>>arr[i];
    }
    print_arry(arr,n);
    for(i=0; i<n; i++){
       cout<<arr[i]<<" ";
    }
    cout<<endl;
return 0;
}
